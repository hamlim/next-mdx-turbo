{
  "name": "@hamstack/next-mdx",
  "version": "0.0.1",
  "description": "TODO",
  "homepage": "TODO",
  "author": {
    "name": "Matthew Hamlin",
    "email": "matthewjameshamlin@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hamlim/next-mdx-turbo",
    "directory": "packages/next-mdx"
  },
  "main": "./dist/es/index.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.ts",
        "default": "./dist/es/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    },
    "./loader": "./loader.cjs"
  },
  "type": "module",
  "devDependencies": {
    "@swc/cli": "0.3.10",
    "@swc/core": "1.4.2",
    "hohoro": "0.3.0",
    "@types/bun": "1.1.11",
    "typescript": "5.4.5",
    "bunchee": "5.5.1",
    "esbuild": "0.24.0"
  },
  "peerDependencies": {
    "@mdx-js/mdx": "^3.1.0",
    "remark-gfm": "^4.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx-frontmatter": "^1.0.0",
    "rehype-mdx-code-props": "^3.0.0"
  },
  "scripts": {
    "xdev": "node --watch-path=./src dev.mjs",
    "xxdev": "bunchee -w",
    "xxxdev": "hohoro",
    "xbuild": "hohoro",
    "xxbuild": "bunchee",
    "xxxbuild": "esbuild ./src/index.ts --bundle --platform=node --target=node22 --outdir=./dist",
    "ybuild": "hohoro",
    "build": "bun ./build.mjs",
    "type-check": "tsc -p ./tsconfig.json",
    "lint": "biome lint ./src/",
    "test": "bun test",
    "prepub": "bun run build && bun run type-check && bun run lint && bun run test",
    "pub": "npm publish --access public"
  },
  "xfiles": ["dist"],
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts"
}
